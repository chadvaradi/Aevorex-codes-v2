{
  "name": "@aevorex/shared-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "dev": "lsof -ti tcp:8083 | xargs -r kill -9 && NODE_OPTIONS='--max-old-space-size=4096' vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "test": "vitest",
    "install-playwright": "playwright install --with-deps",
    "lhci": "lhci",
    "lhci:autorun": "lhci autorun",
    "test:coverage": "vitest run --coverage",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome --headed",
    "coverage:merge": "bash scripts/merge_frontend_coverage.sh",
    "coverage:uncovered": "node ../../scripts/generate_uncovered_frontend.js",
    "coverage:check": "nyc check-coverage --lines 100 --functions 100 --branches 100 --temp-dir shared/frontend/coverage/combined",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:storybook": "storybook test",
    "chromatic": "chromatic --exit-once-uploaded --project-token $CHROMATIC_PROJECT_TOKEN",
    "cy:run:ci": "pnpx concurrently -k \"pnpm dev\" \"wait-on http://localhost:8083 && cypress run --browser chrome --headless\" && pnpm coverage:merge && pnpm coverage:check"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "lightweight-charts": "^4.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.6.3",
    "recharts": "^3.0.2",
    "swr": "^2.3.4"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@chromatic-com/cypress": "^0.11.5",
    "@cypress/code-coverage": "^3.14.5",
    "@lhci/cli": "^0.13.0",
    "@playwright/test": "^1.53.2",
    "@storybook/react-vite": "^8.0.0",
    "@storybook/test-runner": "^0.17.0",
    "@storybook/testing-react": "^1.3.0",
    "@swc/plugin-styled-components": "^8.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.2.0",
    "@types/cypress": "^1.1.3",
    "@types/lodash": "^4.17.20",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "babel-plugin-istanbul": "^6.1.1",
    "chromatic": "^8.0.0",
    "concurrently": "^8.2.2",
    "cypress": "^13.7.0",
    "eslint": "^9.17.0",
    "globals": "^16.3.0",
    "nyc": "^15.1.0",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3",
    "vite": "^5.4.19",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-istanbul": "^7.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8",
    "wait-on": "^8.0.3"
  }
}